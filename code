from random import randint
comp = randint(1, 10)
player = int(input("Guess a number between 1 to 10: "))
cont = 1
while True:
    if player == comp:
        break
    player = int(input(f"No :( You lost. Guess another number between 1 to 10: "))
    cont += 1
print(f"Congratulations, the computer guess {comp}. Before getting it right, you lost {cont} times")
